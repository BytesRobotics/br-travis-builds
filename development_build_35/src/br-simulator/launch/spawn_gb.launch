<launch>

    <arg name="x" default="0.0"/>
    <arg name="y" default="0.0"/>
    <arg name="z" default="0.2"/>

    <include file="$(find br-description)/launch/br_description.launch"/>

    <node name="br_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -model br-v1 -x $(arg x) -y $(arg y) -z $(arg z)" />

</launch>